{
	"name": "@livereader/graphly-d3",
	"version": "1.4.1",
	"description": "Library for creating powerful and interactive graph visualizations with ease.",
	"scripts": {
		"dev": "cd demo && npm start",
		"build": "vite build && vue-tsc --emitDeclarationOnly",
		"format": "prettier --ignore-path .gitignore --write ."
	},
	"exports": {
		".": {
			"import": "./dist/main.es.js",
			"require": "./dist/main.cjs.js"
		},
		"./component/vue3": {
			"import": "./dist/component-vue3.es.js",
			"require": "./dist/components-vue3.cjs.js"
		},
		"./style.css": "./dist/style.css"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/lib/main.d.ts"
			],
			"component/vue3": [
				"dist/components/vue3/index.d.ts"
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LiveReader/graphly-d3.git"
	},
	"author": "LiveReader GmbH (https://livereader.com)",
	"contributors": [
		"Jason Rietzke <jason.rietzke@livereader.de> (https://github.com/jason-rietzke)",
		"Luisa Schmidt (https://github.com/luisa-schmidt)"
	],
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/LiveReader/graphly-d3/issues"
	},
	"homepage": "https://docs.graphly.dev",
	"files": [
		"dist"
	],
	"dependencies": {
		"ajv": "^8.11.2",
		"d3": "^7.6.1"
	},
	"devDependencies": {
		"@types/d3": "^7.4.0",
		"@types/node": "^17.0.41",
		"@vitejs/plugin-vue": "^4.0.0",
		"prettier": "^2.6.2",
		"sass": "^1.58.3",
		"tsc": "^2.0.4",
		"typescript": "^4.9.5",
		"vite": "^4.1.4",
		"vue": "^3.2.47",
		"vue-tsc": "^1.2.0"
	},
	"directories": {
		"lib": "lib"
	},
	"keywords": [
		"d3",
		"d3-visualization",
		"svg",
		"graph",
		"visualization",
		"data-driven-design",
		"graphly-d3",
		"vue",
		"vue-component"
	]
}
