{
	"name": "@livereader/graphly-d3-vue",
	"version": "1.0.0",
	"description": "A Vue component library implementing Graphly D3",
	"main": "./dist/graphly-d3-vue.umd.js",
	"module": "./dist/graphly-d3-vue.es.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "vite build && vue-tsc --emitDeclarationOnly",
		"format": "prettier --ignore-path .gitignore --write ."
	},
	"exports": {
		".": {
			"import": "./dist/graphly-d3-vue.es.js",
			"require": "./dist/graphly-d3-vue.umd.js"
		},
		"./style.css": "./dist/style.css"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LiveReader/graphly-d3-vue.git"
	},
	"author": "LiveReader GmbH (https://livereader.com)",
	"contributors": [
		"Jason Rietzke <jason.rietzke@livereader.de> (https://github.com/jason-rietzke)"
	],
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/LiveReader/graphly-d3-vue/issues"
	},
	"homepage": "https://graphly-d3.livereader.com",
	"files": [
		"dist"
	],
	"dependencies": {
		"@livereader/graphly-d3": "^1.0.0",
		"vue": "^3.2.25"
	},
	"devDependencies": {
		"@types/node": "^18.0.0",
		"@vitejs/plugin-vue": "^2.3.3",
		"prettier": "^2.7.1",
		"typescript": "^4.5.4",
		"vite": "^2.9.9",
		"vue-tsc": "^0.34.7"
	},
	"directories": {
		"src": "src"
	}
}
